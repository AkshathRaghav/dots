{
  "Subsection": {
    "prefix": ["ss"],
    "body": ["\\subsection{$1}"],
    "description": "Subsection"
  },
  "Subsubsection": {
    "prefix": ["sss"],
    "body": ["\\subsubsection{$1}"],
    "description": "Subsubsection"
  },
  "Center": {
    "prefix": ["ce"],
    "body": ["\\begin{center}\n    \\item $1 \n\\end{center}"],
    "description": "Center environment"
  },
  "Center Math": {
    "prefix": ["cm"],
    "body": ["\\[ $1 \\]"],
    "description": "Center Math"
  },
  "Enumeration with capital letters": {
    "prefix": ["enumA"],
    "body": [
      "\\begin{enumerate}[label=\\Alph*.]\n    \\item $1 \n\\end{enumerate}"
    ],
    "description": "Enumeration with capital letters"
  },
  "Enumeration with lowercase letters": {
    "prefix": ["enuma"],
    "body": [
      "\\begin{enumerate}[label=\\alph*.]\n    \\item $1 \n\\end{enumerate}"
    ],
    "description": "Enumeration with lowercase letters"
  },
  "Itemize with no bullets": {
    "prefix": ["itemn"],
    "body": ["\\begin{itemize}[label=]\n    \\item $1 \n\\end{itemize}"],
    "description": "Itemize with no bullets"
  },
  "Full set of parentheses": {
    "prefix": ["le"],
    "body": ["\\left($1\\right)"],
    "description": "Full set of parentheses"
  },
  "Term Table": {
    "prefix": ["ttab"],
    "body": [
      "\\begin{center}\n    \\begin{tabular}{| c | c |}\n        \\hline\n        \\textbf{Term} & \\textbf{Definition} \\\\\\ \\hline \\hline\n        $1 & $2 \\\\\\ \\hline$3\n    \\end{tabular}\n\\end{center}"
    ],
    "description": "Term Table"
  },
  "Variable Table": {
    "prefix": ["vtab"],
    "body": [
      "\\begin{center}\n    \\begin{tabular}{| c | c |}\n        \\hline\n        \\textbf{Variable} & \\textbf{Definition} \\\\\\ \\hline \\hline\n        $1 & $2 \\\\\\ \\hline$3\n    \\end{tabular}\n\\end{center}"
    ],
    "description": "Variable Table"
  },
  "Row": {
    "prefix": ["re"],
    "body": ["$1 & $2 \\\\\\ \\hline$3"],
    "description": "Row"
  },
  "Inline Math": {
    "prefix": ["ie"],
    "body": ["$ $1 $"],
    "description": "Inline Math"
  },
  "Mathmatical Solution": {
    "prefix": ["sol"],
    "body": ["\\begin{center}\n    \\item \\textit{Sol.}\n\\end{center}\n"],
    "description": "Mathmatical Solution"
  },
  "Question": {
    "prefix": ["qe"],
    "body": ["\\section{}\n    \\subsection{Question}\n        $1\n    \\subsection{Work \\& Answer}\n        "],
    "description": "Question"
  },
  "Question Sub": {
    "prefix": ["qes"],
    "body": ["\\subsection{}\n    \\subsubsection{Question}\n        $1\n    \\subsubsection{Work \\& Answer}\n        "],
    "description": "Question inside of a section"
  },
  "Plain Enumeration": {
    "prefix": ["enum"],
    "body": ["\\begin{enumerate}\n    \\item $1 \n\\end{enumerate}"],
    "description": "Plain Enumeration"
  },
  "Plain Text in Math Mode": {
    "prefix": ["rm"],
    "body": ["\\textrm{$1}"],
    "description": "Plain Text in Math Mode"
  },
  "Bold text": {
    "prefix": ["bf"],
    "body": ["\\textbf{$1}"],
    "description": "Bold text"
  },
  "Right Arrow": {
    "prefix": ["ira"],
    "body": ["$ \\rightarrow $"],
    "description": "Right Arrow"
  },
  "Angle": {
    "prefix": ["an"],
    "body": ["$ \\measuredangle $1^{\\circ} $"],
    "description": "A measured angle"
  },
  "Celcius": {
    "prefix": ["ct"],
    "body": ["$1 C$ ^{\\circ} $"],
    "description": "A normal text celcius snippet"
  },
  "Fahrenheit": {
    "prefix": ["ft"],
    "body": ["$1 F$ ^{\\circ} $"],
    "description": "A normal text fahrenheit snippet"
  },
  "Kelvin": {
    "prefix": ["kt"],
    "body": ["$1 K$ ^{\\circ} $"],
    "description": "A normal text kelvin snippet"
  },
  "y axis": {
    "prefix": ["yaxis"],
    "body": ["$ y $-axis"],
    "description": "Just the word y-axis with math mode for y"
  },
  "x axis": {
    "prefix": ["xaxis"],
    "body": ["$ x $-axis"],
    "description": "Just the word x-axis with math mode for x"
  },
  "icos": {
    "prefix": ["icos"],
    "body": ["$ \\cos \\theta $"],
    "description": "cos reference in normal text"
  },
  "isin": {
    "prefix": ["isin"],
    "body": ["$ \\sin \\theta $"],
    "description": "sin reference in normal text"
  },
  "itan": {
    "prefix": ["itan"],
    "body": ["$ \\tan \\theta $"],
    "description": "tan reference in normal text"
  },
  "cos": {
    "prefix": ["cos"],
    "body": ["\\cos \\theta"],
    "description": "cos with theta"
  },
  "sin": {
    "prefix": ["sin"],
    "body": ["\\sin \\theta"],
    "description": "sin with theta"
  },
  "tan": {
    "prefix": ["tan"],
    "body": ["\\tan \\theta"],
    "description": "tan with theta"
  },
  "icsc": {
    "prefix": ["icsc"],
    "body": ["$ \\csc \\theta $"],
    "description": "csc reference in normal text"
  },
  "isec": {
    "prefix": ["isec"],
    "body": ["$ \\sec \\theta $"],
    "description": "sec reference in normal text"
  },
  "icot": {
    "prefix": ["icot"],
    "body": ["$ \\cot \\theta $"],
    "description": "cot reference in normal text"
  },
  "csc": {
    "prefix": ["csc"],
    "body": ["\\csc \\theta"],
    "description": "csc with theta"
  },
  "sec": {
    "prefix": ["sec"],
    "body": ["\\sec \\theta"],
    "description": "sec with theta"
  },
  "cot": {
    "prefix": ["cot"],
    "body": ["\\cot \\theta"],
    "description": "cot with theta"
  },
  "Right Arrow in Math": {
    "prefix": ["ra"],
    "body": ["\\rightarrow"],
    "description": "Right Arrow in Math"
  },
  "Practice Problem": {
    "prefix": ["pra"],
    "body": [
      "\\section{}",
      "\t\\subsection{Work \\& Answer}",
      "\t\t$2",
      "\t\\subsection{Result}",
      "\t\t$3",
      "\t\\subsection{Correction/Take Away}",
      "\t\t$4",
      "",
      "\\horizontalrule"
    ],
    "description": "Practice Problem"
  }
}

